@model IEnumerable<ShoppyMcShopFace.Models.Product>

<h1>Products from CSV</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Image URL</th>
            <th>Price</th>
            <th>Stock</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.ImageURL</td>
                <td>@product.Price</td>
                <td>@product.Stock</td>
                <td>@product.Description</td>
            </tr>
        }
    </tbody>
</table>
